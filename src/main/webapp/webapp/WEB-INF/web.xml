<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <!--add code about new servlet-->
    <servlet>
        <servlet-name>ConfigDemoServlet</servlet-name><!--give any name-->
        <servlet-class>com.GongWenhui.week4.demo.ConfigDemoServlet</servlet-class><!--package + name of servlet class-->
        <init-param>
        <param-name>name</param-name>
        <param-value>GongWenhui</param-value>
    </init-param>
        <init-param>
        <param-name>studentID</param-name>
        <param-value>2019211001000926</param-value>
    </init-param>
    </servlet>
    <servlet-mapping>
    <servlet-name>ConfigDemoServlet</servlet-name>
    <url-pattern>/config</url-pattern>
</servlet-mapping>
    <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.GongWenhui.week3.demo.RegisterServlet</servlet-class>
</servlet>
    <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
</servlet-mapping>
    <context-param>
    <param-name>driver</param-name>
    <param-value/>
</context-param>
    <context-param>
    <param-name>url</param-name>
    <param-value/>
</context-param>
    <context-param>
    <param-name>username</param-name>
    <param-value>GongWenhui</param-value>
</context-param>
    <context-param>
    <param-name>password</param-name>
    <param-value>123456</param-value>
</context-param>
</web-app>